  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "node-fetch": "^2.7.0",
    "papaparse": "^5.5.2"
  }
}
